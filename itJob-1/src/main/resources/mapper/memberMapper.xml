<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.five.member.mapper.memberMapper">
	<!-- 일반회원 회원가입 쿼리 -->
  <insert id="joinN" parameterType="com.five.member.entity.memberNVO">
	insert into member_normal
	values (mem_nom_seq, #{memNomName}, #{memNomId}, #{memNomPw}, #{memNomBirth}, #{memNomEmail}, #{memNomPhone}, #{memNomPlace}, 1)
  </insert>
  
 	<!-- 일반회원 로그인 쿼리 -->
  <select id="loginN" parameterType="com.five.member.entity.memberNVO" resultType="com.five.member.entity.memberNVO">
  	select *
  	from member_normal
  	where mem_nom_id = #{memNomId}
  	and mem_nom_pw = #{memNomPw}
  </select>
  
	<!-- 기업회원 회원가입 쿼리 -->
  <insert id="joinE" parameterType="com.five.member.entity.memberEVO">
	insert into member_ent
	values (mem_ent_seq, #{memEntName}, #{memEntId}, #{memEntPw}, #{memEntEmail}, #{memEntPhone}, #{memEntNum}, #{memEntPlace})
  </insert>
  
	<!-- 기업회원 로그인 쿼리 -->
  <select id="loginE" parameterType="com.five.member.entity.memberEVO" resultType="com.five.member.entity.memberEVO">
  	select *
  	from member_ent
  	where mem_ent_id = #{memEntId}
  	and mem_ent_pw = #{memEntPw}
  </select>
</mapper>